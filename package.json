    "command-line-usage": "^5.0.5",
    "config": "^2.0.1",
    "ini": "^1.3.5",
    "prompt": "^1.0.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "bluebird": "^3.5.2",
    "depcheck": "^0.6.9",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^2.6.0",
    "mocha": "^4.1.0",
    "nyc": "^12.0.2",
    "prettier": "1.13.4",
    "semver": "^5.5.1"
